<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DatosDeConfirmacion">

    <TextView
        android:id="@+id/txt_tel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txt_personas"
        android:layout_marginTop="0dp"
        android:text="TextView"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/tv_datos"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/tv_datos"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/txt_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_datos"
        android:layout_marginTop="1dp"
        android:text="TextView"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/txt_surname"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txt_name"
        android:layout_marginTop="0dp"
        android:text="TextView"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/txt_fecha"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txt_surname"
        android:layout_marginTop="0dp"
        android:text="TextView"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/txt_personas"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txt_fecha"
        android:layout_marginTop="0dp"
        android:text="TextView"
        android:textSize="30sp" />

    <Button
        android:id="@+id/button"
        android:layout_width="126dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txt_tel"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="130dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="155dp"
        android:gravity="center"
        android:onClick="Salir"
        android:text="@string/bt_salir" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button"
        android:layout_marginTop="31dp"
        android:text="@string/tv_text"
        android:textSize="18sp" />

    <Button
        android:id="@+id/call1"
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView6"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="22dp"
        android:layout_marginEnd="10dp"
        android:onClick="llamada33"
        android:text="@string/call1" />

    <Button
        android:id="@+id/Call2"
        android:layout_width="211dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView6"
        android:layout_alignParentStart="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="22dp"
        android:layout_marginEnd="21dp"
        android:layout_toStartOf="@+id/call1"
        android:onClick="llamada87"
        android:text="@string/Call2" />

</RelativeLayout>